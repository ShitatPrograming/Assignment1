 void caesar(char *code, int *key)
 {
   for(i=0;i<200;i++)
  {
      if(code[i]>65 && code[i]<90)//if it is a captial letter
      {
      
      code[i]=(code[i]-65+key)%26+65;
      }
   }
 }
 
 char decaesar(char *code, int *key)
 {
     int key,i;
      for(key=0;key<27;key++)
   {
  for(i=0;i<200;i++)
  {
      if(code[i]>65 && code[i]<90)//if it is a captial letter
      {
      
      decoded[i]=(code[i]-key);//%26+65;
      if(decoded[i]<65)
      { 
        decoded[i]=decoded[i]+26;
      }
  }

   }
   char decaesark(char code, int key*)
   {
        int i ;//index
   int key; //need size of
  for(i=0;i<200;i++)
  {
      if(code[i]!=32 && code[i]!=95 && code[i]!=58 && code[i]!=46)//if it is a captial letter
      {
      
      code[i]=(code[i]-key);//%26+65;
      if(code[i]<65)
      { 
        code[i]=code[i]+26;
      }
      else if (code[i]>65)
      {
          code[i]=code[i];
      }
     
}
}
 }

void sub(char *decaesar)
{
    // subs in individual vaules of the sting

     

    int i; 
if(code[i]>65 && code[i]<90)
{
    for(i=0;i<200;i++) 

    { 

        switch (decaesar[i]) 

